{
  "expo": {
    "name": "FutManager",
    "slug": "futmanager",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#1B5E20"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.futmanager.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1B5E20"
      },
      "package": "com.futmanager.app"
    },
    "web": {
      "bundler": "metro"
    }
  }
}
